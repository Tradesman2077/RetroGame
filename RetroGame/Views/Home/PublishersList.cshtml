@model IEnumerable<Publisher> 

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary text-dark" style="font-family: 'Bebas Neue', cursive;">Publishers</h2>
        </div>
    </div>

    <br /><br />

    @if (Model.Count() > 0)
    {
        <table class="table table-bordered table-striped " style="width:100%">

            <tbody>
                @foreach (var obj in Model)
                {
                    <tr>
                        <td width="100%">@obj.Name</td>

                    </tr>
                }
            </tbody>
        </table>
    }
    else
    {
        <p>No Publishers added</p>
    }
    <div class="row pt-4">
        <div class="col-6">
            <a asp-action="Index" class="btn btn-warning">Back</a>
        </div>
    </div>
</div>

